l_script_id = "a503f2db07e8e325dab3be78be33407d";
local name = 'luarmor__fastload__tmp__'..l_script_id..'.lua'
l_fastload_enabled = function(op)
    if op == 'flush' then
        writefile(name, game:HttpGet("https://api.luarmor.net/files/v3/l/"..l_script_id..".lua"))
    end
    l_fastload_lua()
end
l_fastload_lua = function()    
    local j = {pcall(readfile, name)}
    local s, t = unpack(j)
    if not s then l_fastload_enabled('flush') return end
    loadstring(t)(j)
end
l_fastload_lua()
